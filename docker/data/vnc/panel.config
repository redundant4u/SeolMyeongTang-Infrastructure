[layout]
object-id-list=['menu-bar', 'indicators', 'show-desktop', 'window-list', 'workspace-switcher', 'clock', 'user-menu']
toplevel-id-list=['top-panel', 'bottom-panel']

[layout/objects/clock]
object-iid='org.gnome.gnome-panel.clock::clock'
pack-index=0
pack-type='center'
toplevel-id='top-panel'

[layout/objects/indicators]
object-iid='org.ayatana.indicator-applet::indicator-complete'
pack-index=0
pack-type='end'
toplevel-id='top-panel'

[layout/objects/menu-bar]
object-iid='org.gnome.gnome-panel.menu::menu-bar'
pack-index=0
toplevel-id='top-panel'

[layout/objects/show-desktop]
object-iid='org.gnome.gnome-panel.wncklet::show-desktop'
pack-index=0
toplevel-id='bottom-panel'

[layout/objects/user-menu]
object-iid='org.gnome.gnome-panel.menu::user-menu'
pack-index=1
pack-type='end'
toplevel-id='top-panel'

[layout/objects/window-list]
object-iid='org.gnome.gnome-panel.wncklet::window-list'
pack-index=1
toplevel-id='bottom-panel'

[layout/objects/workspace-switcher]
object-iid='org.gnome.gnome-panel.wncklet::workspace-switcher'
pack-index=0
pack-type='end'
toplevel-id='bottom-panel'

[layout/toplevels/bottom-panel]
alignment='center'
animation-speed='fast'
auto-hide=false
auto-hide-size=1
enable-buttons=false
expand=true
hide-delay=300
monitor=0
orientation='bottom'
size=24
unhide-delay=100

[layout/toplevels/top-panel]
alignment='center'
animation-speed='fast'
auto-hide=false
auto-hide-size=1
enable-buttons=false
expand=true
hide-delay=300
monitor=0
orientation='top'
size=24
unhide-delay=100
